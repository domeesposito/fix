<div id="white-label-header">
  <header id="header" data-hook>
    <div class="container">
      <div class="row">
        <div class="col-xs-4">
          <%= render :partial => 'spree/shared/nav_bar' %>
        </div>
        <figure id="logo" class="col-xs-4" data-hook>
          <%= logo %>
        </figure>
        <div class="col-xs-4">
          <%= render :partial => 'spree/shared/main_nav_bar' if store_menu? %>
        </div>
      </div>
    </div>
  </header>
</div>
